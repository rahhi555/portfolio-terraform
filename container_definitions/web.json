[
  {
    "name": "svg_portfolio_web",
    "image": "299271904246.dkr.ecr.ap-northeast-1.amazonaws.com/svg-portfolio-web:latest",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/web",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "ecs-web"
      }
    },
    "secrets": [
      {
        "name": "FRONT_URL",
        "valueFrom": "/web/front-url"
      },
      {
        "name": "RAILS_DB_HOST",
        "valueFrom": "/db/hostname"
      },
      {
        "name": "APP_DATABASE_PASSWORD",
        "valueFrom": "/db/raw/password"
      },
      {
        "name": "RAILS_MASTER_KEY",
        "valueFrom": "/web/rails-master-key"
      }
    ]
  }
]