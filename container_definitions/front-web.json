[
  {
    "name": "svg_portfolio_front",
    "image": "299271904246.dkr.ecr.ap-northeast-1.amazonaws.com/svg-portfolio-front:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/front",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "ecs-front"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "secrets": [
      {
        "name": "BASE_URL",
        "valueFrom": "/front/baseURL"
      },
      {
        "name": "BROWSER_BASE_URL",
        "valueFrom": "/front/browserBaseURL"
      },
      {
        "name": "SERVER_MIDDLEWARE_URL",
        "valueFrom": "/front/serverMiddlewareUrl"
      }
    ]
  },
  {
    "name": "svg_portfolio_web",
    "image": "299271904246.dkr.ecr.ap-northeast-1.amazonaws.com/svg-portfolio-web:latest",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/web",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "ecs-web"
      }
    },
    "secrets": [
      {
        "name": "RAILS_MASTER_KEY",
        "valueFrom": "/web/rails-master-key"
      }
    ]
  }
]